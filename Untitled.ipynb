   "source": [
    "import numpy as np\n",
    "\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1.0/(1.0 + np.exp(-x))\n",
    "\n",
    "inputs = [13, 3, -12]\n",
    "\n",
    "for x in inputs:\n",
    "    print('{} --> {}'.format(x, sigmoid(x)))\n",
    "\n"
   ]
